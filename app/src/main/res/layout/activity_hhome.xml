<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.mohammedalsamawi.fabe.Hhome"
    android:background="#000000"
    android:columnCount="2"
    android:rowCount="3"
    >
<GridLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:id="@+id/lecturersid"
        android:background="@drawable/licon"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_marginBottom="5dp"
        android:clickable="true"
        android:onClick="Lecturerspage" />
    <ImageView
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:id="@+id/Contactsid"
        android:background="@drawable/cicon"
        android:layout_row="0"
        android:layout_column="1"
        android:layout_marginLeft="8dp"
        android:onClick="Contactpage" />
    <ImageView
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:id="@+id/timetableid"
        android:background="@drawable/ticon"
        android:layout_row="1"
        android:layout_column="0"
        android:layout_marginBottom="5dp"
        android:onClick="Timetablepage" />
    <ImageView
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:id="@+id/imageView3"
        android:background="@drawable/dicon"
        android:layout_row="1"
        android:layout_column="1"
        android:layout_marginLeft="8dp"
        android:onClick="Calendarpage"/>

    <ImageView
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:id="@+id/gallaryid"
        android:background="@drawable/gicon"
        android:layout_row="2"
        android:layout_column="0"
        android:onClick="Gallerypage"/>
    <ImageView
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:id="@+id/aboutbutton"
        android:background="@drawable/aicon"
        android:layout_row="2"
        android:layout_column="1"
        android:layout_marginLeft="8dp"
        android:onClick="Aboutpage"/>
    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_row="2"
        android:layout_column="0">

    </GridLayout>
</GridLayout>
</RelativeLayout>
